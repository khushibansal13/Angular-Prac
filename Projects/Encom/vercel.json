{
  "version": 2,
  "builds": [
    {
      "src": "Projects/Encom/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/encom/browser",
        "skipCache": true
}
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}

