{
  "version": 2,
  "builds": [
    {
      "src": "Projects/Encom/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/encom/browser",
        "buildCommand": "cd Projects/Encom && npm run build",
        "skipCache": true
      }
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/browser/index.html" }
  ]
}
